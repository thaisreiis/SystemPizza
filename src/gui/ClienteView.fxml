<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="366.0" prefWidth="648.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ClientesController">
   <children>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
        <items>
          <Button fx:id="btNovoCliente" mnemonicParsing="false" onAction="#onBtNovoClienteAction" text="Novo Cliente" />
        </items>
      </ToolBar>
      <ToolBar prefHeight="40.0" prefWidth="200.0">
         <items>
            <TextField fx:id="txtPesquisar" prefHeight="25.0" prefWidth="537.0" promptText="pesquisar cliente" />
         </items>
      </ToolBar>
      <TableView fx:id="tableViewClientes" prefHeight="251.0" prefWidth="648.0">
        <columns>
          <TableColumn fx:id="tableColumnNome" prefWidth="103.0" text="Nome" />
          <TableColumn fx:id="tableColumnSobrenome" prefWidth="262.0" text="Sobrenome" />
            <TableColumn fx:id="tableColumnTelefone" prefWidth="101.0" text="Telefone" />
            <TableColumn fx:id="tableColumnEDIT" prefWidth="75.0" />
            <TableColumn fx:id="tableColumnREMOVE" prefWidth="75.0" />
        </columns>
      </TableView>
   </children>
</VBox>
